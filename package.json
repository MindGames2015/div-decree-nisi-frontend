{
  "name": "divorce-frontend-boilerplate",
  "description": "Divorce frontend boilerplate",
  "version": "0.0.1",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "NODE_PATH=. NODE_ENV=development node-dev --inspect=127.0.0.1:9229 app.js",
    "test:unit": "NODE_PATH=. NODE_ENV=testing nyc --reporter=text-summary mocha --exit 'test/unit/**/*.js'",
    "test:e2e": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=OFF NODE_PATH=. codeceptjs run -c test/e2e/codecept.conf.js --steps --invert",
    "lint": "NODE_PATH=. eslint ."
  },
  "dependencies": {
    "@hmcts/look-and-feel": "^1.13.2",
    "@hmcts/nodejs-healthcheck": "^1.4.6",
    "@hmcts/nodejs-logging": "^3.0.0",
    "@hmcts/one-per-page": "^3.3.2",
    "@hmcts/one-per-page-test-suite": "https://github.com/hmcts/one-per-page-test-suite.git",
    "copy-webpack-plugin": "^4.5.1",
    "express": "^4.16.3",
    "glob": "^7.1.2",
    "helmet": "^3.12.0",
    "i18next": "^11.3.1",
    "ioredis": "^3.2.2",
    "joi": "^13.3.0",
    "moment": "^2.22.1",
    "os": "^0.1.1",
    "paths": "^0.1.1",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "@hmcts/div-idam-test-harness": "https://github.com/hmcts/div-idam-test-harness.git",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-jq": "^0.0.9",
    "codeceptjs": "^1.1.8",
    "domino": "^2.0.2",
    "eslint": "^4.19.1",
    "mocha": "^5.1.1",
    "mochawesome": "^3.0.2",
    "node-dev": "^3.1.3",
    "nyc": "^11.7.2",
    "puppeteer": "^1.4.0",
    "randomstring": "^1.1.5",
    "require-directory": "^2.1.1",
    "sinon": "^5.0.7",
    "sinon-chai": "^3.0.0",
    "supertest": "^3.0.0",
    "zepto-node": "^1.0.0"
  }
}
